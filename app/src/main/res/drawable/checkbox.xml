<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:drawable="@drawable/ic_checkbox_disabled_focused"
          android:state_focused="true"
          android:state_enabled="false"
          android:state_checked="false" />
    <item android:drawable="@drawable/ic_checkbox_disabled"
          android:state_focused="false"
          android:state_enabled="false"
          android:state_checked="false" />
    <item android:drawable="@drawable/ic_checkbox_disabled_checked"
          android:state_focused="false"
          android:state_enabled="false"
          android:state_checked="true" />
    <item android:drawable="@drawable/ic_checkbox_focused"
          android:state_focused="true"
          android:state_enabled="true"
          android:state_checked="false" />
    <item android:drawable="@drawable/ic_checkbox_focused_checked"
          android:state_focused="true"
          android:state_enabled="true"
          android:state_checked="true" />
    <item
        android:drawable="@drawable/ic_checkbox_pressed_checked"
        android:state_checked="true"
        android:state_pressed="true"/>
    <item
        android:drawable="@drawable/ic_checkbox_pressed"
        android:state_checked="false"
        android:state_pressed="true"/>
    <item
        android:drawable="@drawable/ic_checkbox_normal_checked"
        android:state_checked="true"/>
    <item
        android:drawable="@drawable/ic_checkbox_normal" />


</selector>